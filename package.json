    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lhci": "^4.1.1",
    "lighthouse": "^12.8.1",
    "lint-staged": "^16.1.5",
    "playwright": "^1.54.2",
    "start-server-and-test": "^2.0.13",
    "vite": "^7.1.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^1.0.2",
    "vitest": "^3.2.4"
  }
}
