  "scripts": {
    "dev": "vite --port 5173 --open",
    "start": "vite --port 5173 --open",
    "start:headless": "vite --port 5173",
    "test": "npm run unit && npm run e2e && npm run test:pwa",
    "e2e": "BROWSERSLIST_IGNORE_OLD_DATA=1 start-server-and-test start:headless http://localhost:5173 'cypress run --config video=false'",
    "e2e:open": "BROWSERSLIST_IGNORE_OLD_DATA=1 start-server-and-test start:headless http://localhost:5173 'cypress open'",
    "build": "vite build",
    "preview": "vite preview",
    "unit": "vitest run",
    "unit:watch": "vitest",
    "lint": "eslint src --ext .js,.jsx --fix --max-warnings=0",
    "lint:check": "eslint src --ext .js,.jsx --max-warnings=0",
    "lint:staged": "lint-staged",
    "test:pwa": "playwright test tests/minimal-pwa-test.spec.js",
    "test:pwa:ui": "playwright test --ui",
    "test:pwa:debug": "playwright test --debug",
    "test:browserstack": "node tests/browserstack.js",
    "lighthouse": "lighthouse http://localhost:5173 --output=json --output-path=./lighthouse-report.json",
    "lighthouse:ci": "lhci autorun"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@vitejs/plugin-react": "^5.0.0",
    "browserstack-local": "^1.5.8",
